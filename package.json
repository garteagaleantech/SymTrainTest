{
  "name": "sym-train-test",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "postinstall": "npx husky install",
    "build": "tsc",
    "start": "npm run compile && node dist/",
    "compile": "shx rm -rf dist/ && tsc && npm run post:compile",
    "post:compile": "tscpaths -p tsconfig.json -s ./src -o ./dist --silent",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/",
    "format": "prettier . -w",
    "lint": "tsc && prettier -c . && eslint . --ext .js,.ts -f codeframe"
  },
  "type": "commonjs",
  "keywords": [],
  "author": "Gerardo arteaga",
  "license": "ISC",
  "devDependencies": {
    "@baemingo/tscpaths-async": "0.0.15",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.15.0",
    "sequelize-cli": "^6.4.1",
    "shx": "^0.3.4"
  }
}
